	           class="com.surelogic.sierra.tool.eclipse.NewSierraToolProjectWizard"
	           project="true"
	           id="com.surelogic.sierra.tool.eclipse.newproject">
	      <description>Create a Sierra tool project.</description>
	   </wizard>
	</extension>
</plugin>
